<!DOCTYPE html>
<html lang="en">

<head>
    {{> head }}
</head>

<body>

    {{> body }}
    {{> footer}}

    <!--JAVASCRIPT-->
    {{> scripts}}


    {{#if page.maps}}
	    <!--maps-->
	        {{> gmap page.maps}}
    {{/if}}


	{{#if page.carousels}}
		<!--carousels-->
		<script type="text/javascript" src="{{assets}}js/vendor/slick.min.js"></script>
		{{#forEach page.carousels }}
			<script type="text/javascript" src="{{path}}"></script>
		{{/forEach}}
	{{/if}}


	{{#if page.charts}}
		<!--charts-->
		<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.4.0/Chart.bundle.js"></script>
		{{#forEach page.charts }}
			<script type="text/javascript" src="{{path}}"></script>
		{{/forEach}}
	{{/if}}

</body>

</html>
